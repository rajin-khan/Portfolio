{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/send-newsletter\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/subscribe\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/test-redis\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/unsubscribe\\/?$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "/404.html",
			"status": 404
		}
	]
}